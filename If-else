import java.util.Scanner;
public class vote {
	public static void main(String args[]){
		Scanner sys=new Scanner (System.in);
		
		System.out.print("Enter The Age : ");
		int age=sys.nextInt();
		
		if(age>=18){
			System.out.println("You Can Vote...");			
		}
		else
			System.out.println("You Cannot Vote...");
	}
			

}
